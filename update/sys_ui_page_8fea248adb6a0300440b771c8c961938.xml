        <category>general</category>
        <client_script><![CDATA[jQuery("#addDog").click(function(){
	
	var ga = new GlideAjax('fetchUtils');
	
	ga.addParam('sysparm_name', 'createDog');
	ga.addParam('sysparm_dog_name', jQuery('#dogName').val());
	ga.addParam('sysparm_dog_age', jQuery('#dogAge').val());
	ga.addParam('sysparm_dog_neutered', jQuery('#dogNeutured').is(':checked'));
	ga.addParam('sysparm_dog_shots', jQuery('#dogShots').is(':checked'));
	ga.getXML(fetchCallback);
	
});

function fetchCallback(response) {
	var answer = response.responseXML.documentElement.getAttribute("answer").split('|');
	//jQuery('#msg').remove('.successMessage');
	jQuery('#msg').append('<p class="successMessage">' + answer[0] + answer[1] +answer[2] +answer[3] +' added successfully!</p>');
	//alert(answer);
}]]></client_script>
        <description/>
        <direct>false</direct>
        <endpoint>x_58872_needit_Dog Form.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
	<div class="container">  
		<h2>Add Dog</h2>
		<form class="form-horizontal">
			<div class="form-group">
				<label class="control-label col-sm-2" for="name">Dog Name</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="fa fa-user">@</i></span>
					<input type="text" class="form-control" placeholder="Dog Name" name="name" id="dogName"/>
				</div>
			</div>
			<div class="form-group">
				<label class="control-label col-sm-2" for="age">Dog Age</label>
				<div class="input-group">
					<span class="input-group-addon"><i class="fa fa-car">@</i></span>
					<input type="text" class="form-control" placeholder="Dog Age" name="age" id="dogAge"/>
				</div>
			</div>
			<div class="form-group">        
				<div class="col-sm-offset-2 col-sm-10">
					<div class="checkbox">
						<label><input type="checkbox" name="neutured" id="dogNeutured"/>Neutured</label>
					</div>
					<div class="checkbox">
						<label><input type="checkbox" name="shots" id="dogShots"/>Shots</label>
					</div>
				</div>
			</div>
		</form>
		<div class="form-group">        
			<div class="col-sm-offset-2 col-sm-10">
				<button class="btn btn-default" id="addDog">Submit</button>
			</div>
		</div>
	</div>
	<pre id="msg"></pre>
</j:jelly>]]></html>
